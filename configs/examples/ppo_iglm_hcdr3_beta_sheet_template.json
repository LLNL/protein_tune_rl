{
    "trainer": {
        "name": "online_rl_trainer",
        "batch_size": 8,
        "max_length": 22,
        "check_point_freq": 2000,
        "total_optimization_steps": 10
    },
    "experiment_directory": "test_run",
    "dataset": {
        "name": "infilling",
        "data_directory": "data/examples/training_data_v3.csv",
        "chain": "HC",
        "region": "HCDR3"
    },
    "tokenizer": {
        "name": "iglm_tokenizer",
        "tokenizer_config": "${IGLM_DIR}",
        "padding_side": "left"
    },
    "collator": {
        "name": "infilling"
    },
    "policy_model": {
        "name": "iglm",
        "dir": "${IGLM_DIR}"
    },
    "KL_penalty": {
        "weight": 0.01,
        "target": null
    },
    "metric": [
        {
            "name": "ss_perc_sheet",
            "params": {
                "mean": 0.3784639359426365,
                "std": 0.017445864484741506
            }
        }
    ],
    "optimizer": {
        "name": "ppo",
        "learning_rate": 1e-05,
        "entropy_weight": 0.0,
        "normalize_advantage": true,
        "baseline": "mean",
        "minibatch_size": 2
    }
}